{
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;database=BlogexplorerDB;trusted_connection=true;TrustServerCertificate=True"
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:7111",
        "Certificate": {
          "Path": "C://Windows//System32/localhost+2.pem",
          "KeyPath": "C://Windows//System32/localhost+2-key.pem"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
